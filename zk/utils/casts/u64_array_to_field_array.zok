import "utils/casts/u64_to_bits" as u64_to_bits
import "utils/pack/bool/pack128" as pack128

def main<N,P>(u64[N] hash) -> field[P]:
    assert(N == 2 * P)

    field[P] hash_field = [0; P]

    for u32 i in 0..P do
        hash_field[i] = pack128([
            ...u64_to_bits(hash[2 * i + 0]),
            ...u64_to_bits(hash[2 * i + 1])
        ])
    endfor

    return hash_field
